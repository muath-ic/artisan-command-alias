{
    "name": "muath-ic/artisan-command-alias",
    "description": "Define aliases for Laravel Artisan commands",
    "homepage": "https://github.com/muath-ic/artisan-command-alias",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "illuminate/console": "5.6.*",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "muath-ic\\ArtisanAlais\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "muath-ic\\ArtisanAlais\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "muath-ic\\ArtisanAlais\\ArtisanCommandAliasServiceProvider"
            ]
        }
    }
}
